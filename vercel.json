{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/download",
        "dest": "index.js",
        "methods": ["POST"]
      },
      {
        "src": "/",
        "dest": "index.js",
        "methods": ["GET"]
      }
    ]
  }